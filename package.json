{
  "name": "fleet-manager-e2e",
  "private": true,
  "scripts": {
  "cy:open": "cypress open",
  "cy:run": "cypress run --reporter cypress-mochawesome-reporter --reporter-options \"reportDir=reports,json=true,html=true,overwrite=false\"",
  "report:prep": "node -e \"require('fs').mkdirSync('reports/json-backup',{recursive:true})\"",
  "report:merge": "npx mochawesome-merge reports/json-backup/*.json > reports/mochawesome.json",
  "report:html": "npx mochawesome-report-generator reports/mochawesome.json -f index -o reports/html",
  "report": "npm run report:merge && npm run report:html",
  "full-report": "npm run cy:run && npm run report"
},
  "devDependencies": {
    "cypress": "^13.13.0",
    "cypress-mochawesome-reporter": "^3.8.4",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "typescript": "^5.4.0"
  }
}